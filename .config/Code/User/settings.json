{
    "gitlens.advanced.messages": {
        "suppressShowKeyBindingsNotice": true
    },
    "workbench.colorCustomizations": {
        "editor.selectionBorder": "#ffff00",
        "editor.selectionHighlightBorder": "#ffff00",
        "editor.findMatchBorder": "#ff0000",
        "editor.findMatchHighlightBorder": "#ff0000",
        "editor.findMatchHighlightBackground": "#000000"
    },
    "editor.accessibilitySupport": "off",
    "editor.acceptSuggestionOnEnter": "smart",
    "editor.colorDecorators": false,
    "editor.cursorStyle": "line",
    "editor.cursorWidth": 3,
    "editor.cursorBlinking": "smooth",
    "editor.foldingStrategy": "indentation",
    "editor.fontFamily": "Iosevka",
    "editor.fontSize": 16,
    "editor.letterSpacing": 0,
    "editor.minimap.maxColumn": 80,
    "editor.minimap.showSlider": "always",
    "editor.minimap.renderCharacters": false,
    "editor.quickSuggestionsDelay": 50,
    "editor.renderLineHighlight": "gutter",
    "editor.snippetSuggestions": "top",
    "editor.scrollBeyondLastLine": false,
    "editor.tabCompletion": true,
    "editor.tabSize": 2,
    "scm.diffDecorationsGutterWidth": 5,
    "workbench.commandPalette.preserveInput": true,
    "workbench.editor.enablePreviewFromQuickOpen": false,
    "workbench.editor.labelFormat": "short",
    "workbench.editor.tabCloseButton": "off",
    "workbench.startupEditor": "newUntitledFile",
    "workbench.statusBar.feedback.visible": false,
    "window.enableMenuBarMnemonics": false,
    "window.newWindowDimensions": "maximized",
    "window.restoreFullscreen": true,
    "files.insertFinalNewline": true,
    "files.trimFinalNewlines": true,
    "files.trimTrailingWhitespace": true,
    "files.useExperimentalFileWatcher": true,
    "search.smartCase": true,
    "search.quickOpen.includeSymbols": true,
    "debug.inlineValues": true,
    "telemetry.enableTelemetry": false,
    "scrolloff.scrolloff": 5,
    "togglequotes.chars": [
        "\"",
        "'",
        "`"
    ],
    "window.zoomLevel": 0,
    "workbench.editor.showTabs": true,
    "editor.minimap.enabled": true,
    "workbench.activityBar.visible": false,
    "gitlens.advanced.telemetry.enabled": false,
    "gitlens.codeLens.authors.enabled": false,
    "local-history.path": "~/.vscode_localhistory/",
    "shellcheck.exclude": [
        2116, // useless echo
        2164, // cd
    ],
    "tslint.autoFixOnSave": true,
    "breadcrumbs.enabled": true,
    "typescript.updateImportsOnFileMove.enabled": "always",
    "eslint.alwaysShowStatus": true,
    "prettier.endOfLine": "lf",
    "extensions.ignoreRecommendations": false,
    "editor.renderWhitespace": "none",
    "window.menuBarVisibility": "toggle",
    "prettier.disableLanguages": [
        "javascript",
        "jsx"
    ],
    "prettier.printWidth": 120,
    "editor.cursorSurroundingLines": 5,
    "eslint.autoFixOnSave": true,
    "files.exclude": {
        "**/.vscode_localhistory": true
    },
    "gitlens.defaultDateStyle": "absolute",
    "gitlens.hovers.currentLine.over": "line",
    "gitlens.currentLine.enabled": false,
    "gitlens.statusBar.format": "${author}   ${agoOrDate}   ${tips}   ",
    "gitlens.hovers.currentLine.details": false,
    "gitlens.keymap": "alternate",
    "gitlens.mode.statusBar.alignment": "left",
    "gitlens.codeLens.enabled": false,
    "vsicons.dontShowNewVersionMessage": true,
    "eslint.run": "onSave",
}
