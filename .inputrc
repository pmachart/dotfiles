set completion-ignore-case on
Tab: menu-complete
set show-all-if-ambiguous on
set menu-complete-display-prefix on

"\e[1;5C": forward-word
"\e[1;5D": backward-word
"\e[5C": forward-word
"\e[5D": backward-word
"\e\e[C": forward-word
"\e\e[D": backward-word
"\e[3;5~": shell-kill-word
"\C-H": shell-backward-kill-word

set meta-flag on
set convert-meta off
set output-meta on
